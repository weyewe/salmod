<% source = object.one_to_one_source %> 
<% target = object.one_to_one_target %>
<tr> 
	<td><%= object.code %></td>
	<td><b><%= source.quantity %>x</b> <%= source.item.name  %></td>
	<td><b><%= target.quantity %>x</b>  <%= target.item.name %></td>
	<td>
		<%= object.convert_stocks.where(:is_deleted => false ).count%>
	</td>
	
	
	<td>
		<%= link_to new_stock_conversion_convert_stock_url(object) , :class => "new_entry" do %>
			<i class="icon-plus"></i>Create
		<% end %>
	</td>

	
</tr>